@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(foxtrotCore REQUIRED)
find_dependency(foxtrotProtocols REQUIRED)
find_dependency(rttr 0.9.6 REQUIRED)

if(EXISTS ${CMAKE_CURRENT_LIST_DIR}/installtree)
    message(WARNING "foxtrotDevicesConfig configuring from BUILD TREE! You may not want this!")
    set(BUILD_TREE TRUE)
else()
    message(STATUS "foxtrotDevicesConfig configuring from install tree (normal behaviour)")
    set(BUILD_TREE FALSE)
endif()


set(FOXTROT_DEVICES_BUILTIN "@device_targets@")


if(BUILD_TREE)
    include(${CMAKE_CURRENT_LIST_DIR}/foxtrotDevicesexports.cmake)
    set_property(TARGET foxtrot::foxtrot_devices APPEND PROPERTY
                 INTERFACE_INCLUDE_DIRECTORIES ${CMAKE_CURRENT_LIST_DIR})
else()
    if(NOT TARGET foxtrot::foxtrot_devices AND NOT foxtrot_devices_BINARY_DIR)
        include(@PACKAGE_ft_cmakedir@/foxtrot_devices.cmake)
    endif()
endif()

check_required_components(foxtrot_devices)
