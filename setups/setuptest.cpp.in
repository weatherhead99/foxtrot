#include "ExperimentalSetup.h"
#include <iostream>
#include "DeviceHarness.h"

#include "backward.hpp"

using std::cout;
using std::endl;

int main(int argc, char** argv)
{
    backward::SignalHandling sh;
    foxtrot::DeviceHarness harness;
    foxtrot::ExperimentalSetup setup("@TEST_SETUP_FILE@",harness);
    
    
    auto devmap = harness.GetDevMap();
    
    cout << "devices in harness.." << endl;
    
    for(auto& item: devmap)
    {
     cout << "id: " << item.first << "\t";
     cout << "name: " << item.second->getDeviceTypeName() << endl;
        
    }
    
    auto capnames = harness.GetCapabilityNames(1);
    for(auto& cap : capnames)
    {
        cout << "capability name:" << cap << endl;
    };
    
    
    


}
